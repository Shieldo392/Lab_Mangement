<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lab_management">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_icon"
        android:label="Quản lý phòng Lab"
        android:roundIcon="@drawable/logo_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">
        <activity android:name=".manage_labs.EditLabActivity" android:screenOrientation="portrait"></activity>
        <activity android:name=".manage_labs.AddLabActivity" android:screenOrientation="portrait"/>
        <activity android:name=".manage_labs.ManageLabs" android:screenOrientation="portrait"/>
        <activity android:name=".manage_registerlab.RegisterLabActivity" android:screenOrientation="portrait"/>
        <activity android:name=".login.SignUp" android:screenOrientation="portrait"/>
        <activity android:name=".manage_device.EditDevice" android:screenOrientation="portrait"/>
        <activity android:name=".manage_device.ListDevice" android:screenOrientation="portrait"/>
        <activity android:name=".manage_device.AddDeviceActivity" android:screenOrientation="portrait"/>
        <activity android:name=".manage_verify.ListVerify" android:screenOrientation="portrait"/>
        <activity android:name=".manage_verify.EditVerify" android:screenOrientation="portrait"/>
        <activity android:name=".manage_verify.AddNewVerify" android:screenOrientation="portrait"/>
        <activity
            android:name=".login.LoginActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" android:screenOrientation="portrait"/>
    </application>

</manifest>